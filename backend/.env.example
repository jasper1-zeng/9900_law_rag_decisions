# LLM API Keys
OPENAI_API_KEY=your_openai_api_key_here
DEEPSEEK_API_KEY=your_deepseek_api_key_here

# LLM Provider Configuration
CHAT_LLM_PROVIDER=openai  # Options: openai, deepseek
CHAT_LLM_MODEL=gpt-4o  # For OpenAI: gpt-4o, gpt-3.5-turbo
ARGUMENTS_LLM_PROVIDER=deepseek  # Options: openai, deepseek
ARGUMENTS_LLM_MODEL=deepseek-reasoner

# LLM Settings
LLM_TEMPERATURE=0.2
LLM_MAX_TOKENS=4096
ENABLE_STREAMING=true

# Database Configuration
DATABASE_URL=postgresql://username:password@localhost/satdata
DB_HOST=localhost
DB_PORT=5432
DB_NAME=satdata
DB_USER=username
DB_PASSWORD=password

# RAG Settings
EMBEDDING_MODEL=e5-base-v2
RELEVANCE_THRESHOLD=0.7

# Neo4j API Settings
NEO4J_API_BASE_URL=http://localhost:5001
NEO4J_URI=neo4j+s://your-instance-id.databases.neo4j.io
NEO4J_USER=neo4j
NEO4J_PASSWORD=your_neo4j_password 